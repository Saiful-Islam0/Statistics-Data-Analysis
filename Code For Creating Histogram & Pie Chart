Q.5 Have you ever felt guilty or ashamed after consuming pornography?

import matplotlib.pyplot as plt

data = [
    "Yes", "Yes", "Yes", "Yes", "Yes", "Yes", "Yes", "Yes", "Yes", "Yes", 
    "Yes", "Yes", "Yes", "Yes", "No", "Yes", "Yes", "No", "No", "Yes", 
    "Yes", "Yes", "Yes", "Yes", "Yes", "Yes", "Yes", "No", "Yes", "Yes", 
    "Yes", "Yes", "Yes", "No", "Yes", "Yes", "No", "No", "Yes", "No", 
    "Yes", "Yes", "Yes", "Yes", "Yes", "No", "Yes", "No", "Yes", "No", 
    "Yes", "Yes", "Yes", "Yes", "No", "Yes", "No", "Yes", "No", "Yes", 
    "Yes", "Yes", "Yes", "No", "Yes", "No", "Yes", "Yes", "No", "Yes", 
    "Yes", "No", "Yes", "Yes", "Yes", "No", "Yes", "Yes", "No", "Yes", 
    "No", "Yes", "Yes", "Yes", "Yes", "No", "No", "Yes", "Yes", "Yes", 
    "Yes", "Yes", "Yes", "Yes", "Yes", "Yes", "Yes", "No", "Yes", "Yes", 
    "Yes", "Yes", "Yes", "Yes", "Yes", "Yes", "Yes", "Yes", "Yes", "Yes", 
    "Yes", "Yes", "Yes", "No", "Yes", "Yes", "Yes", "Yes", "Yes", "Yes", 
    "Yes", "Yes", "Yes", "Yes", "Yes", "Yes", "Yes", "Yes", "Yes", "Yes", 
    "Yes", "Yes", "Yes", "Yes", "Yes", "Yes", "Yes", "Yes", "Yes", "Yes", 
    "Yes", "Yes", "Yes", "No", "Yes", "Yes", "No", "Yes", "Yes", "No", 
    "Yes", "Yes", "Yes", "Yes", "Yes", "Yes", "No", "No", "Yes", "Yes", 
    "Yes", "Yes", "Yes", "Yes", "Yes", "Yes", "Yes", "Yes", "Yes", "Yes", 
    "Yes", "Yes", "Yes", "No", "Yes", "Yes", "Yes", "Yes", "Yes", "Yes", 
    "No", "Yes", "Yes", "Yes", "No", "Yes", "Yes", "Yes", "Yes", "Yes", 
    "Yes", "Yes", "Yes", "Yes", "Yes", "Yes", "Yes", "Yes", "Yes", "Yes", 
    "Yes", "No", "Yes", "Yes", "Yes", "Yes", "Yes", "Yes", "Yes", "Yes"
]
feeling_counts = {"Yes": 0, "No": 0}
for feeling in data:
    if feeling == "Yes":
        feeling_counts["Yes"] += 1
    else:
        feeling_counts["No"] += 1

labels = list(feeling_counts.keys())
values = list(feeling_counts.values())

colors = ['lightblue', 'lightgreen']
plt.figure(figsize=(10, 6))
plt.pie(values, labels=labels, colors=colors, autopct='%1.1f%%', startangle=140)
plt.title('Pie Chart of Guilt or Shame after Consuming Pornography')
plt.axis('equal')  # Equal aspect ratio ensures that pie is drawn as a circle.
plt.show()

plt.figure(figsize=(10, 6))
plt.hist(data, color=['lightblue', 'lightgreen'])
plt.xlabel('Feeling after Consuming Pornography')
plt.ylabel('Frequency')
plt.title('Histogram of Guilt or Shame after Consuming Pornography')
plt.xticks(rotation=45, ha='right')
plt.tight_layout()
plt.show()

