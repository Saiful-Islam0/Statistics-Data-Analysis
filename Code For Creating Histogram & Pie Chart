import matplotlib.pyplot as plt
data = [
    "Men", "Both", "Both", "Men", "Both", "Men", "No significant difference", 
    "Both", "Men", "Men", "Both", "Both", "Both", "Both", "Men", "Men", 
    "Both", "Both", "Both", "Men", "Men", "Men", "Men", "Men", 
    "No significant difference", "No significant difference", "Men", "Men", 
    "Men", "Both", "Both", "Both", "Both", "Men", "Both", "Men", "Both", 
    "Both", "Men", "Men", "Both", "Men", "Men", "Men", "Both", "Men", 
    "Both", "Both", "Both", "Both", "Both", "Men", "No significant difference", 
    "Men", "Both", "Men", "Both", "Men", "Men", "No significant difference", 
    "Both", "Both", "Men", "Both", "Men", "Both", "Men", "Both", 
    "No significant difference", "Both", "Both", "Men", "Both", "Both", 
    "Men", "Both", "Men", "Men", "Both", "Both", "Men", "Both", "Both", 
    "No significant difference", "Both", "Men", "Both", "Both", "Women", 
    "Both", "Both", "Both", "Men", "Both", "No significant difference", 
    "Men", "Both", "No significant difference", "Both", "Both", 
    "No significant difference", "Men", "Women", "Both", "Men", "Men", 
    "Men", "Men", "Both", "Both", "Men", "No significant difference", 
    "Men", "No significant difference", "Both", "Men", "Men", "Men", 
    "Men", "Men", "Men", "Men", "Men", "Men", "Both", "No significant difference", 
    "Both", "Men", "Men", "Men", "Women", "Both", "No significant difference", 
    "Both", "Both", "Both", "Men", "Men", "No significant difference", 
    "Men", "Both", "Both", "Both", "Both", "Both", "Both", "Men", "Both", 
    "Men", "Both", "Men", "No significant difference", "Men", "Men", 
    "Both", "Men", "Men", "No significant difference", "Men", 
    "No significant difference", "No significant difference", "Men", "Men"
]

addiction_counts = {}
for addiction in data:
    if addiction in addiction_counts:
        addiction_counts[addiction] += 1
    else:
        addiction_counts[addiction] = 1

labels = list(addiction_counts.keys())
values = list(addiction_counts.values())
colors = ['lightblue', 'lightgreen', 'lightcoral', 'lightyellow', 'lightskyblue']

